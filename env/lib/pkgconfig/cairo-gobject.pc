prefix=/home/franco/projects/test/ucp_project/env
includedir=${prefix}/include
libdir=${prefix}/lib

Name: cairo-gobject
Description: cairo-gobject for cairo graphics library
Version: 1.18.0
Requires: cairo, glib-2.0 >=  2.14, gobject-2.0
Requires.private: zlib, libpng, fontconfig >=  2.2.95, freetype2 >=  9.7.3, x11, xext, xrender >=  0.6, xcb >=  1.6, xcb-render >=  1.6, x11-xcb, xcb-shm, pixman-1 >=  0.36.0
Libs: -L${libdir} -lcairo-gobject
Libs.private: -lm -ldl
Cflags: -I${includedir}/cairo
